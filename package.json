{
  "name": "swimtrack",
  "version": "0.2.0",
  "description": "SwimTrack Trainingsplattform mit Node.js Backend",
  "main": "backend/server.js",
  "scripts": {
    "start": "node backend/server.js",
    "dev": "nodemon backend/server.js",
    "seed": "node backend/seed.js --reset",
    "lint": "eslint .",
    "test:backend": "node --test tests/backend",
    "test:frontend": "vitest run",
    "test:integration": "node --test tests/api.test.js",
    "test": "npm run lint && npm run test:backend && npm run test:frontend && npm run test:integration"
  },
  "keywords": [
    "swimming",
    "training",
    "dashboard"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "better-sqlite3": "^9.0.0",
    "cors": "^2.8.5",
    "express": "^4.18.2"
  },
  "devDependencies": {
    "eslint": "^8.57.0",
    "jsdom": "^24.0.0",
    "nodemon": "^3.0.3",
    "vitest": "^1.5.0"
  }
}
